    <applicationSettings>
        <Gopherit.My.MySettings>
            <setting name="StyleDefault" serializeAs="String">
                <value>body {
  /* page background color: */
  background-color: white;
  /* color for unknown selectors: */
  color: black;
}
pre {
  /* all gophermap text */
  font-family: monospace;
 /* you can replace "monospace" with whatever font you desire surrounded by ' characters */
}
.i {
  /* inline text */
  color: green;
}
.h {
  /* html document link */
  color: blue;
}
.0 a {
  /* text file */
  color: gray;
}
.1 a {
  /* directory */
  color: orange;
}
.7 a {
  /* search service */
  color: purple;
}
.3 a {
  /* error */
  color: red;
}
.5 a {
  /* PC binary */
  color: brown;
}
.9 a {
  /* binary file */
  color: brown;
}
.g a {
  /* GIF image */
  color: brown;
}
.Im a {
  /* gopher item type capital i */
  /* generic image */
  color: brown;
}</value>
            </setting>
        </Gopherit.My.MySettings>
    </applicationSettings>
